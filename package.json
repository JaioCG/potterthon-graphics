{
  "name": "potterthon",
  "description": "On-screen graphics for Potterthon events",
  "homepage": "https://twitch.tv/potterthon",
  "version": "1.0.0",
  "files": [
    "dashboard",
    "graphics",
    "extension.js",
    "extension"
  ],
  "keywords": [
    "",
    "nodecg-bundle"
  ],
  "nodecg": {
    "compatibleRange": "^2.0.0",
    "dashboardPanels": [
        {
            "name": "commentary-control",
